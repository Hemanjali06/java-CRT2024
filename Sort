import java.util.*;
 class Sort 
{
    public static void main(String[] args)
{
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        int i=0;
        for(i=n-1;i>=n/2;i--)
        {
            System.out.print(a[i]+" ");
        }
        for(int j=0;j<n/2;j++)
        {
            System.out.print(a[j]+" ");
        }
    }
}
